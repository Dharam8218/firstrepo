<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" id="WebApp_ID" version="4.0">
  <display-name>MiniProjectDevelopment</display-name>
  <welcome-file-list>
    <welcome-file>input.html</welcome-file>
    
  </welcome-file-list>
  <servlet>
      <servlet-name>bill</servlet-name>
      <servlet-class>com.bytecode.servlet.CalculateBillServlet</servlet-class>
    </servlet>
    <servlet-mapping>
       <servlet-name>bill</servlet-name>
       <url-pattern>/billurl</url-pattern>
    </servlet-mapping>
     <servlet>
      <servlet-name>discount</servlet-name>
      <servlet-class>com.bytecode.servlet.DiscountServlet</servlet-class>
    </servlet>
    <servlet-mapping>
       <servlet-name>discount</servlet-name>
       <url-pattern>/discounturl</url-pattern>
    </servlet-mapping>
  </web-app>