<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" id="WebApp_ID" version="4.0">
  <display-name>ServletToDBApp</display-name>
  <welcome-file-list>
    <welcome-file>input.html</welcome-file>
   
  </welcome-file-list>
  <servlet>
      <servlet-name>db1</servlet-name>
      <servlet-class>com.bytecode.servlet.ServletToDB</servlet-class>
  </servlet>
  <servlet-mapping>
      <servlet-name>db1</servlet-name>
      <url-pattern>/test</url-pattern>
  </servlet-mapping>
   <servlet>
      <servlet-name>db2</servlet-name>
      <servlet-class>com.bytecode.servlet.HeaderServlet</servlet-class>
  </servlet>
  <servlet-mapping>
      <servlet-name>db2</servlet-name>
      <url-pattern>/headerurl</url-pattern>
  </servlet-mapping>
   <servlet>
      <servlet-name>db3</servlet-name>
      <servlet-class>com.bytecode.servlet.ErrorServlet</servlet-class>
  </servlet>
  <servlet-mapping>
      <servlet-name>db3</servlet-name>
      <url-pattern>/errorurl</url-pattern>
  </servlet-mapping>
</web-app>